<script>
$(document).ready(function(){
    //NPF photosets script by annasthms
    undoPhotoset({
        'posts': '.text', 
        'text class': 'caption',
        'photoset class': 'photoset'
    },true); 
    //pxu photosets by @ shythemes & bychloethemes
    $('.photo-slideshow').pxuPhotoset({
        lightbox: true,
        rounded: false,
        gutter: '{select:photoset gutter}px',
        photoset: '.photo-slideshow',
        photoWrap: '.photo-data',
        photo: '.pxu-photo'
    });
     // timeago plugin by @ bychloethemes
        $('.timestamp').timeAgo({
        time: "short",
        spaces: true,
        words: false,
        suffix: "ago",  
        });
        (function($){ $(document).ready(function(){
        $("a[title]").style_my_tooltips({
        tip_follows_cursor:true,
        tip_delay_time:10,
        tip_fade_speed:200,
        attribute:"title"});
    });
    })(jQuery);
      // audio player fix by @ shudesigns
        var $audio = $('iframe.tumblr_audio_player');
        $audio.load(function() {
        $(this).contents().find('head').append('<style type="text/css">' +
      '.audio-player { background: {color:posts}; color: {color:links} !important; }' +
      '.audio-player .audio-info .track-artist { color: {color:links} !important; }' +
    '</style>');
  });
});
</script>

