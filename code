<!-----------Time ago script by bychloethemes @ tumblr----------->
<script type="text/javascript" src="https://static.tumblr.com/i5s2zks/9Acok8oo2/bct-timeago.min.js"></script>
<!-----------Time ago script by bychloethemes @ tumblr----------->
 
<script>
$(document).ready(function(){
    $('.photo-slideshow').pxuPhotoset({
        lightbox: true,
        rounded: false,
        gutter: '{select:photoset gutter}px',
        photoset: '.photo-slideshow',
        photoWrap: '.photo-data',
        photo: '.pxu-photo'
    });
    {block:IndexPage}
    var $container = $('#content');
    $container.masonry({ itemSelector: '.posts' });
    $container.imagesLoaded(function(){
    $container.masonry();
    $container.find('.posts').animate({ opacity: 1, zIndex: 1 });
    });
    {/block:IndexPage}
    $('.timestamp').timeAgo({
        time: "word",
        spaces: true,
        words: true,
        suffix: "ago",  
    });
  // audio player fix by @ shudesigns
    var $audio = $('iframe.tumblr_audio_player');
        $audio.load(function() {
        $(this).contents().find('head').append('<style type="text/css">' +
      '.audio-player { background: {color:posts}; color: {color:links} !important; }' +
      '.audio-player .audio-info .track-artist { color: {color:links} !important; }' +
    '</style>');
    });
});
</script>

